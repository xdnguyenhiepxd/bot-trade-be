  "scripts": {
    "dev": "nest start --watch",
    "prebuild": "rimraf dist",
    "build": "nest build && npm run postbuild",
    "format": "prettier --write .",
    "postbuild": "copyfiles -u 2 src/telegram/images/**/* dist/telegram",
