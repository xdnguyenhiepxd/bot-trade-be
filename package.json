    "@nestjs/swagger": "^6.1.4",
    "@project-serum/anchor": "^0.26.0",
    "@solana/web3.js": "^1.92.3",
    "@types/bcrypt": "^5.0.2",
    "@types/passport-jwt": "^4.0.1",
    "bcrypt": "^5.1.1",
    "bs58": "^5.0.0",
