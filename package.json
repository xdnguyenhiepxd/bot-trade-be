    "dotenv": "^16.4.5",
    "imagekit": "^5.0.1",
    "mongoose": "^6.8.2",
    "node-telegram-bot-api": "^0.66.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "process": "^0.11.10",
    "promise-retry": "^2.0.1",
